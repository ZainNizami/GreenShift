{
  "name": "greenshift",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "install-client": "npm install --prefix client",
    "install": "npm run install-client",
    "client": "npm start --prefix client",
    "flask-window": "cd server && .venv\\Scripts\\activate && flask run",
    "flask-mac": "cd server && source .venv/bin/activate && flask run",
    "dev-window": "concurrently \"npm run client\" \"npm run flask-window\"",
    "dev-mac": "concurrently \"npm run client\" \"npm run flask-mac\""
  },
